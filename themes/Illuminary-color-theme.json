/*
       /\
      /  \
     /,--.\
    /< () >\
   /  `--'  \
  /__________\

  Illuminary Theme (Dark) by Aerobird98

  Colors used:

  #939597 Ultimate Gray          -> #97999B (Pantone Cool Gray 7 C)
  #F5DF4D Illuminating (Yellow)  -> #FAE053 (Pantone 113 C)

  #4ABBD5 Bachelor Button (Blue) -> #4EC3E0 (Pantone 637 C)
  #F2F0EB Snow White             -> #D9D9D6 (Pantone Cool Gray 1 C)
  #E96393 Sangria Sunset (Pink)
  #F38554 Autumn Sunset (Orange) -> #F87C56 (Pantone 2024 C)
  #C1DB3C Love Bird (Green)      -> #C0DF16 (Pantone 2297 C)

  #1D1F2A Rich Black (Pantone 532 C)
  #101820 Rich Black (Pantone Black 6 C)

*/

{
  "name": "Illuminary",
  "tokenColors": [
    {
      "name": "String",
      "scope": "string",
      "settings": {
        "foreground": "#4ABBD5"
      }
    },
    {
      "name": "String Escape",
      "scope": "constant.character.escape, text.html constant.character.entity.named, punctuation.definition.entity.html",
      "settings": {
        "foreground": "#4ABBD5",
        "fontStyle": "bold"
      }
    },
    {
      "name": "Boolean",
      "scope": "constant.language.boolean",
      "settings": {
        "foreground": "#F38554"
      }
    },
    {
      "name": "Number",
      "scope": "constant.numeric",
      "settings": {
        "foreground": "#F38554"
      }
    },
    {
      "name": "Identifier",
      "scope": "variable, support.variable, support.class, support.constant, meta.definition.variable entity.name.function",
      "settings": {
        "foreground": "#F2F0EB"
      }
    },
    {
      "name": "Keyword",
      "scope": "keyword, modifier, variable.language.this, support.type.object, constant.language",
      "settings": {
        "foreground": "#939597",
        "fontStyle": "bold",
      }
    },
    {
      "name": "Function call",
      "scope": "entity.name.function, support.function",
      "settings": {
        "foreground": "#F5DF4D"
      }
    },
    {
      "name": "Storage",
      "scope": "storage.type, storage.modifier",
      "settings": {
        "foreground": "#4ABBD5",
        "fontStyle": "bold"
      }
    },
    {
      "name": "Modules",
      "scope": "support.module, support.node",
      "settings": {
        "foreground": "#4ABBD5",
        "fontStyle": "italic"
      }
    },
    {
      "name": "Type",
      "scope": "support.type, entity.name.type, entity.other.inherited-class",
      "settings": {
        "foreground": "#F5DF4D",
        "fontStyle": "bold"
      }
    },
    {
      "name": "Comment",
      "scope": "comment",
      "settings": {
        "foreground": "#939597",
        "fontStyle": "italic"
      }
    },
    {
      "name": "Class",
      "scope": "entity.name.type.class",
      "settings": {
        "foreground": "#F5DF4D",
        "fontStyle": "bold"
      }
    },
    {
      "name": "Class variable",
      "scope": "variable.object.property, meta.field.declaration entity.name.function",
      "settings": {
        "foreground": "#F5DF4D",
      }
    },
    {
      "name": "Class method",
      "scope": "meta.definition.method entity.name.function",
      "settings": {
        "foreground": "#F5DF4D",
      }
    },
    {
      "name": "Function definition",
      "scope": "meta.function entity.name.function",
      "settings": {
        "foreground": "#F5DF4D"
      }
    },
    {
      "name": "Template expression",
      "scope": "template.expression.begin, template.expression.end, punctuation.definition.template-expression.begin, punctuation.definition.template-expression.end",
      "settings": {
        "foreground": "#939597"
      }
    },
    {
      "name": "Reset embedded/template expression colors",
      "scope": "meta.embedded, source.groovy.embedded, meta.template.expression",
      "settings": {
        "foreground": "#F2F0EB"
      }
    },
    {
      "name": "YAML key",
      "scope": "entity.name.tag.yaml",
      "settings": {
        "foreground": "#4ABBD5"
      }
    },
    {
      "name": "JSON key",
      "scope": "meta.object-literal.key, meta.object-literal.key string, support.type.property-name.json",
      "settings": {
        "foreground": "#939597"
      }
    },
    {
      "name": "JSON constant",
      "scope": "constant.language.json",
      "settings": {
        "foreground": "#939597"
      }
    },
    {
      "name": "CSS class",
      "scope": "entity.other.attribute-name.class",
      "settings": {
        "foreground": "#939597",
        "fontStyle": "bold"
      }
    },
    {
      "name": "CSS ID",
      "scope": "entity.other.attribute-name.id",
      "settings": {
        "foreground": "#4ABBD5",
        "fontStyle": "bold"
      }
    },
    {
      "name": "CSS tag",
      "scope": "source.css entity.name.tag",
      "settings": {
        "foreground": "#F5DF4D",
        "fontStyle": "bold"
      }
    },
    {
      "name": "HTML tag outer",
      "scope": "meta.tag, punctuation.definition.tag",
      "settings": {
        "foreground": "#F2F0EB"
      }
    },
    {
      "name": "HTML tag inner",
      "scope": "entity.name.tag",
      "settings": {
        "foreground": "#F5DF4D"
      }
    },
    {
      "name": "HTML tag attribute",
      "scope": "entity.other.attribute-name",
      "settings": {
        "foreground": "#F5DF4D"
      }
    },
    {
      "name": "Markdown heading",
      "scope": "markup.heading",
      "settings": {
        "foreground": "#939597"
      }
    },
    {
      "name": "Markdown link text",
      "scope": "text.html.markdown meta.link.inline, meta.link.reference",
      "settings": {
        "foreground": "#4ABBD5",
        "fontStyle": "underline"
      }
    },
    {
      "name": "Markdown list item",
      "scope": "text.html.markdown beginning.punctuation.definition.list",
      "settings": {
        "foreground": "#939597"
      }
    },
    {
      "name": "Markdown italic",
      "scope": "markup.italic",
      "settings": {
        "foreground": "#4ABBD5",
        "fontStyle": "italic"
      }
    },
    {
      "name": "Markdown bold",
      "scope": "markup.bold",
      "settings": {
        "foreground": "#F5DF4D",
        "fontStyle": "bold"
      }
    },
    {
      "name": "Markdown bold italic",
      "scope": "markup.bold markup.italic, markup.italic markup.bold",
      "settings": {
        "foreground": "#C1DB3C",
        "fontStyle": "italic bold"
      }
    },
    {
      "name": "Markdown code block",
      "scope": "markup.fenced_code.block.markdown punctuation.definition.markdown",
      "settings": {
        "foreground": "#4ABBD5"
      }
    },
    {
      "name": "Markdown code block inner",
      "scope": "text.html.markdown markup.fenced_code.block.markdown ",
      "settings": {
        "foreground": "#4ABBD5"
      }
    },
    {
      "name": "Markdown inline code",
      "scope": "markup.inline.raw.string.markdown",
      "settings": {
        "foreground": "#4ABBD5"
      }
    },
    {
      "name": "INI property name",
      "scope": "keyword.other.definition.ini",
      "settings": {
        "foreground": "#4ABBD5"
      }
    },
    {
      "name": "INI section title",
      "scope": "entity.name.section.group-title.ini",
      "settings": {
        "foreground": "#939597"
      }
    },
    {
      "name": "C# class",
      "scope": "source.cs meta.class.identifier storage.type",
      "settings": {
        "foreground": "#F5DF4D",
        "fontStyle": "bold"
      }
    },
    {
      "name": "C# class method",
      "scope": "source.cs meta.method.identifier entity.name.function",
      "settings": {
        "foreground": "#F5DF4D"
      }
    },
    {
      "name": "C# function call",
      "scope": "source.cs meta.method-call meta.method, source.cs entity.name.function",
      "settings": {
        "foreground": "#F5DF4D"
      }
    },
    {
      "name": "C# type",
      "scope": "source.cs storage.type",
      "settings": {
        "foreground": "#F5DF4D",
        "fontStyle": "bold"
      }
    },
    {
      "name": "C# return type",
      "scope": "source.cs meta.method.return-type",
      "settings": {
        "foreground": "#F5DF4D"
      }
    },
    {
      "name": "C# preprocessor",
      "scope": "source.cs meta.preprocessor",
      "settings": {
        "foreground": "#939597"
      }
    },
    {
      "name": "C# namespace",
      "scope": "source.cs entity.name.type.namespace",
      "settings": {
        "foreground": "#4ABBD5"
      }
    },
  ],
